<theme>
    <formatVersion>7</formatVersion>

    <customView name="gamecarousel">
        
        <!-- Video Game PrecustomView: HIDE -->
        <video name="overlay, overlay2">
            <zIndex>0</zIndex>
        </video>

        <!-- Name -->
        <text name="md_name">
            <color>0d723e</color>
        </text>

        <!-- Rating Classificação do Game -->
        <rating name="md_rating">
            <color>0d723e</color>
            <unfilledColor>0d723e</unfilledColor>
        </rating>

    </customView>

</theme>
